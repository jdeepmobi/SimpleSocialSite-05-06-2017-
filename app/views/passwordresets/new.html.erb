
<h2>Password Reset</h2>
<h3>Enter your email</h3>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:passwordresets, url: passwordresets_path ) do |f| %>
     

      <%= f.label :email %><br>
      <%= f.text_field :email, class: 'form-control' %><br><br>


      <%= f.submit "Check", class: "btn btn-primary" %><br>
    <% end %>
    <li><%= link_to "Back", '#' %></li>
    
  </div>
</div>